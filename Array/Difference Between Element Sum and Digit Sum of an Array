class Solution {
    public int differenceOfSum(int[] nums) {
        int total = 0, digit = 0;
        for(int i = 0;i<nums.length;i++){
            int n = nums[i];
            total += nums[i];
            while(n>0){
                digit=digit+n%10;
                n/=10;
            }
        }
        return total-digit;
    }
}
